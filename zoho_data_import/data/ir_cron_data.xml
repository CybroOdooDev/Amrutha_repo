<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <data>
        <record id="ir_cron_scheduler_sync_email" model="ir.cron">
            <field name="name">Zoho Email Fetch</field>
            <field name="model_id" ref="model_zoho_queue"/>
            <field name="state">code</field>
            <field name="code">model.action_sync_zoho_emails()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
<!--            <field name="numbercall">-1</field>-->
<!--            <field name="doall">1</field>-->
        </record>

            <record id="ir_cron_scheduler_fetch_access_token" model="ir.cron">
            <field name="name">Generate Zoho Access Token</field>
            <field name="model_id" ref="model_zoho_connector"/>
            <field name="state">code</field>
            <field name="code">model.generate_access_token()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">45</field>
            <field name="interval_type">minutes</field>
<!--            <field name="numbercall">-1</field>-->
<!--            <field name="doall">1</field>-->
        </record>
    </data>
</odoo>